\n}\r\n\r\n* {\r\n    margin: 0;\r\n    padding: 0;\r\n    color: $color80;\r\n    font-family: Verdana, Geneva, Tahoma, sans-serif;\r\n}\r\n\r\nsection {\r\n    max-width: 1200px;\r\n    margin: 0 auto;\r\n    width: 100%;\r\n    height: 100vh;\r\n}\r\n\r\n.hero {    \r\n    background-image: url(\"../img/pxfuel2.jpg\");\r\n    background-color: $color80;\r\n    background-repeat: no-repeat;\r\n    background-size: cover;\r\n    margin-bottom: 1rem;\r\n}\r\n\r\n.container {\r\n    margin: 0 auto;\r\n    width: 80%;\r\n}\r\n\r\nnav {\r\n    display: flex;\r\n    justify-content: space-between;\r\n    align-items: center;\r\n    padding: 1rem;\r\n    height: 2rem;\r\n\r\n    .logo {\r\n        letter-spacing: 0.25rem;\r\n        line-height: 1.3;\r\n        &::first-letter {\r\n            background-color: $color40;\r\n            padding: 0 1px 0 5px;\r\n        }\r\n    }\r\n\r\n    ul {\r\n        list-style: none;\r\n        display: flex;\r\n        justify-items: flex-end;\r\n        align-items: center;\r\n    }\r\n\r\n    li {\r\n        padding-right: 0.5rem;\r\n        font-size: $small-font;\r\n        \r\n        &:last-child {\r\n            padding-right: 0;\r\n        }\r\n    }\r\n\r\n    a {\r\n        text-decoration: none;       \r\n\r\n    }\r\n}\r\n\r\n.person {\r\n    display: flex;\r\n    height: 60vh;\r\n    justify-content: space-between;\r\n    align-items: center;\r\n    text-align: left;\r\n    padding-top: clamp(6rem, 20vh,7rem);\r\n    \r\n\r\n    \r\n\r\n    img {\r\n        display: block;        \r\n        width: clamp(300px,35vw,500px);\r\n        border: 2px solid $color60;\r\n        align-self: flex-start;\r\n    }\r\n\r\n    .text-info{\r\n        padding-left: 2rem;\r\n        align-self: flex-end;\r\n        text-shadow: 1px 1px 2px $color0;\r\n                \r\n        @mixin blending{\r\n            mix-blend-mode: hard-light;\r\n            \r\n        }\r\n    \r\n        h2 {\r\n            margin-top: 2rem;        \r\n            @include blending;\r\n        }\r\n        h3 {\r\n            margin: 2rem;\r\n            @include blending;\r\n        }\r\n        h1{\r\n            @include blending;\r\n            font-size: 2rem;\r\n        }\r\n    }\r\n}\r\n\r\n.work {\r\n    text-align: center;\r\n    background-color: $color80;\r\n    padding-top: 3rem;\r\n\r\n    h2,\r\n    h4,\r\n    h5,\r\n    p {\r\n        color: $color20;\r\n    }\r\n\r\n    .card-container {\r\n        margin-top: 4rem;\r\n        display: flex;\r\n        flex-direction: row;\r\n        justify-content: center;\r\n        align-items: center;\r\n\r\n        .card {\r\n            background-color: $color100;\r\n            margin: 1rem;\r\n            width: 100vw;\r\n            height: 40vh;\r\n            @include shadow;\r\n            position: relative;\r\n        }\r\n\r\n        img {\r\n            display: block;\r\n            @include shadow;\r\n            width: $small-img-size;\r\n            height: $small-img-size;\r\n            border-radius: 50%;\r\n            border: 2px solid $color100;\r\n            position: absolute;\r\n            top: -($small-img-size/2);\r\n            left: 0;\r\n            right: 0;\r\n            margin-left: auto;\r\n            margin-right: auto;\r\n        }\r\n\r\n        h4 {\r\n            margin-top: 56px;\r\n        }\r\n\r\n        h5 {\r\n            font-style: italic;\r\n            margin: 0.25rem;\r\n        }\r\n        p {\r\n            font-size: $small-font;\r\n        }\r\n    }\r\n\r\n    @media only screen and (max-width: 768px){\r\n        .work{\r\n            height: auto;\r\n        }\r\n        .card-container{\r\n            flex-direction: column;\r\n            justify-content: space-between;\r\n            .card{\r\n                width: 50vw;\r\n                height: auto;\r\n                margin: 3rem 0;\r\n            }\r\n        }\r\n    \r\n    }\r\n\r\n    .indicators {\r\n        display: flex;\r\n        justify-content: center;\r\n        align-items: center;\r\n        margin-top: 1rem;\r\n\r\n        .line {\r\n            margin: 3px;\r\n            width: 25px;\r\n            height: 6px;\r\n            background-color: $color0;\r\n            @include shadow;\r\n        }\r\n\r\n        .active{\r\n            background-color: $color60;\r\n        }\r\n    }\r\n}\r\n\r\n@media only screen and (max-width: 768px){\r\n    section{\r\n        height: auto;\r\n\r\n        .person{\r\n            flex-direction: column;\r\n            padding-top: 0; \r\n            height: auto;        \r\n            padding-bottom: 1rem;  \r\n            text-align: right;\r\n        }\r\n    }\r\n   \r\n}\r\n\r\n\r\n"},"lineCount":null}},"error":null,"hash":"527ba05edfc9889c4db4137c1f8cc5ee","cacheData":{"env":{}}}